//배열 array1.c
#include<stdio.h>

int main(void) {
	int data[10];

	printf("정수 10개 써\n");
	for (int i = 0; i < sizeof(data)/sizeof(int); i++){
	scanf_s("%d", & data[i]);
}
	printf("입력힌 데이터 10개 각자에 +100한 결과\n");
	for (int i = 0; i < sizeof(data)/4; i++){
	printf("data[%d]: %d\n",i ,data[i]+=100);
	}
	return 0;
}
